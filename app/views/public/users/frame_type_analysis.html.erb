<span>身体的特徴について、当てはまる項目を1つずつ選択してください。</span>
<%= form_with url: frame_type_result_user_path(current_user.user_id), method: :post do |f| %>
  <% @frame_questions.each do |q| %>
    <p><%= q.title %></p>
    <%= f.radio_button :"#{q.id}", "A", checked: true %>
    <%= f.label :"#{q.id}", "#{q.strate}" %>
    <%= f.radio_button :"#{q.id}", "B" %>
    <%= f.label :"#{q.id}", "#{q.wave}" %>
    <%= f.radio_button :"#{q.id}", "C" %>
    <%= f.label :"#{q.id}", "#{q.natural}" %>
  <% end %>
  <%= f.submit "結果を見る" %>
<% end %>
